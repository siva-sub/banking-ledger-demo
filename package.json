{
  "name": "general-ledger-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Educational General Ledger System demonstrating financial workflows, ISO 20022 messaging, and MAS 610 regulatory reporting",
  "homepage": "https://siva-sub.github.io/banking-ledger-demo",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "generate-demo-data": "tsx scripts/generate-demo-data.ts",
    "bundle-analyze": "npm run build && npx webpack-bundle-analyzer dist/assets/*.js"
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.0",
    "@ant-design/icons": "^5.2.0",
    "@types/lodash": "^4.14.202",
    "antd": "^5.12.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "fast-xml-parser": "^4.3.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.0",
    "vite": "^4.5.0",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "General Ledger System Team",
  "license": "MIT",
  "keywords": [
    "general-ledger",
    "financial-system",
    "iso-20022",
    "mas-610",
    "banking",
    "compliance",
    "regulatory-reporting",
    "typescript",
    "react",
    "ant-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/siva-sub/banking-ledger-demo.git"
  },
  "bugs": {
    "url": "https://github.com/siva-sub/banking-ledger-demo/issues"
  }
}
