{
  "name": "general-ledger-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Educational General Ledger System demonstrating financial workflows, ISO 20022 messaging, and MAS 610 regulatory reporting",
  "homepage": "https://siva-sub.github.io/banking-ledger-demo",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "bundle-analyze": "npm run build && npx webpack-bundle-analyzer dist/assets/*.js",
    "test": "vitest"
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.0",
    "@ant-design/icons": "^5.2.0",
    "@types/lodash": "^4.14.202",
    "@types/react-window": "^1.8.8",
    "antd": "^5.12.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "fast-xml-parser": "^4.3.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.5.2",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.20.0",
    "react-window": "^1.8.11",
    "recharts": "^3.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-resizable": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.0",
    "vite": "^4.5.0",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "General Ledger System Team",
  "license": "MIT",
  "keywords": [
    "general-ledger",
    "financial-system",
    "iso-20022",
    "mas-610",
    "banking",
    "compliance",
    "regulatory-reporting",
    "typescript",
    "react",
    "ant-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/siva-sub/banking-ledger-demo.git"
  },
  "bugs": {
    "url": "https://github.com/siva-sub/banking-ledger-demo/issues"
  }
}
